FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    U
    {
        solver           CG;
        preconditioner   Cholesky;

        tolerance        1e-06;
        relTol           0;
    }
}

stressedFoam
{
    nCorrectors     50;
    U               1e-07;
}


